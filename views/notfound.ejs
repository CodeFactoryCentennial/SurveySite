<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <link rel="stylesheet" href="/stylesheets/style.css">


    <title>
        Survey Not Found
    </title>
</head>

<body>

    <style>
        .callout {
            padding: 15rem 0;
            height: 100vh;
            background: linear-gradient(90deg, rgba(255, 255, 255, 0.1) 0%, rgba(255, 255, 255, 0.1) 100%), url("/images/headerbg.jpg");
            background-position: center center;
            background-repeat: no-repeat;
            background-size: cover;
        }

        .callout h2,
        .callout .h2 {
            font-size: 3rem;
            font-family: 'Lobster', cursive;
            margin: 0;
            color: whitesmoke;
            padding: 0;
            text-shadow: 0px 0px 8px black;
        }
    </style>

<div style="margin-top: 0;">

    <%- include('partials/navbar'); %>

</div>

    <!-- Callout-->
    <section class="callout">
        <div class="container px-4 px-lg-5 text-center">
            <h2 class="mx-auto mb-2">
                Survey
                <em>Not&nbsp;</em>
                Found
            </h2>
            <h5 style="font-style: italic;" class="text-white mx-auto mb-5"><span style="color: black;">Oops!</span> I think this survey might be deleted or changed.

            </h5>
            <a class="btn btn-primary btn-xl" href="/">
                Go Back to Home
            </a>
        </div>
    </section>

    <!-- Bootstrap -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.10.2/dist/umd/popper.min.js"
        integrity="sha384-7+zCNj/IqJ95wo16oMtfsKbZ9ccEh31eOz1HGyDuCQ6wgnyJNSYdrPa03rtR1zdB"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js"
        integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13"
        crossorigin="anonymous"></script>

    <script src="/reload/reload.js"></script>
    <script src="/javascripts/script.js"></script>

</body>

</html>